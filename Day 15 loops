package com.company;
import java.util.Scanner;
public class loops {
    public static void main(String[] args) {
        int num;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter a Number : ");
        num=sc.nextInt();

        for (int i=1;i<=10;i++)
        {
            System.out.println(i*num);
        }
    }
}
